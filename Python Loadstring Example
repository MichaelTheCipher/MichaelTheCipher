import pandas as pd

# Create a list of data
data = [
    ["Alice", 25, "Female"],
    ["Bob", 30, "Male"],
    ["Carol", 27, "Female"],
    ["Dave", 32, "Male"],
]

# Create a DataFrame
df = pd.DataFrame(data, columns=["Name", "Age", "Gender"])

# Load the string as Python code
loadstring = """
# Print the DataFrame
print(df)
"""

# Execute the code
exec(loadstring)


Name  Age  Gender
0  Alice   25  Female
1    Bob   30    Male
2  Carol   27  Female
3   Dave   32    Male
